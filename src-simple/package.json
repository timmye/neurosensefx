{
  "name": "neurosense-fx-simple",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5175 --host 0.0.0.0 --strict-port",
    "build": "vite build",
    "preview": "vite preview --port 5175 --host 0.0.0.0",
    "build-preview": "vite build && vite preview --port 5175 --host 0.0.0.0",
    "test": "playwright test --config=playwright.config.cjs",
    "test:headed": "playwright test --config=playwright.config.cjs --headed",
    "test:ui": "playwright test --config=playwright.config.cjs --ui",
    "test:console": "playwright test --config=playwright.config.cjs console-logging.spec.js",
    "test:console:headed": "playwright test --config=playwright.config.cjs console-logging.spec.js --headed",
    "test:console:ui": "playwright test --config=playwright.config.cjs --ui console-logging.spec.js"
  },
  "dependencies": {
    "interactjs": "^1.10.27",
    "svelte": "^4.2.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "vite": "^5.4.19"
  }
}
