{
  "name": "extended-session-testing-framework",
  "version": "1.0.0",
  "description": "Production-grade framework for genuine 8+ hour extended session testing with actual memory tracking",
  "main": "ExtendedSessionIntegration.js",
  "type": "module",
  "exports": {
    ".": "./ExtendedSessionIntegration.js",
    "./RealExtendedSessionTester": "./RealExtendedSessionTester.js",
    "./ExtendedSessionMonitor": "./ExtendedSessionMonitor.js",
    "./ProfessionalTradingSimulator": "./ProfessionalTradingSimulator.js",
    "./MemoryLeakDetector": "./MemoryLeakDetector.js",
    "./SessionHealthMonitor": "./SessionHealthMonitor.js",
    "./SessionReporter": "./SessionReporter.js",
    "./SessionOptimizer": "./SessionOptimizer.js"
  },
  "scripts": {
    "demo": "node demo-real-session.js",
    "demo:basic": "node -e \"import('./demo-real-session.js').then(m => m.runBasic8HourSession())\"",
    "demo:highfreq": "node -e \"import('./demo-real-session.js').then(m => m.runHighFrequencySession())\"",
    "demo:memory": "node -e \"import('./demo-real-session.js').then(m => m.runMemoryLeakDetectionSession())\"",
    "demo:trading": "node -e \"import('./demo-real-session.js').then(m => m.runProfessionalTradingSimulation())\"",
    "demo:all": "node -e \"import('./demo-real-session.js').then(m => m.runAllDemos())\"",
    "test:integration": "node -e \"import('./ExtendedSessionIntegration.js').then(m => console.log('✅ Integration test passed'))\"",
    "test:monitor": "node -e \"import('./ExtendedSessionMonitor.js').then(m => console.log('✅ Monitor test passed'))\"",
    "test:detector": "node -e \"import('./MemoryLeakDetector.js').then(m => console.log('✅ Detector test passed'))\"",
    "validate": "node -e \"Promise.all([import('./ExtendedSessionIntegration.js'), import('./RealExtendedSessionTester.js'), import('./MemoryLeakDetector.js')]).then(() => console.log('✅ All modules validated'))\""
  },
  "keywords": [
    "testing",
    "extended-session",
    "memory-leak",
    "performance",
    "trading",
    "monitoring",
    "professional",
    "8-hour",
    "real-time"
  ],
  "author": "NeuroSense FX",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "no-dependencies": "pure-javascript-implementation"
  },
  "devDependencies": {
    "no-dev-dependencies": "self-contained-framework"
  },
  "peerDependencies": {
    "browser-environment": ">=90.0",
    "node-environment": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neurosensefx/neurosensefx",
    "directory": "tests/extended-session"
  },
  "bugs": {
    "url": "https://github.com/neurosensefx/neurosensefx/issues"
  },
  "homepage": "https://github.com/neurosensefx/neurosensefx/tree/main/tests/extended-session#readme",
  "files": [
    "*.js",
    "*.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "sideEffects": false
}