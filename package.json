{
  "name": "neurosense-fx",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "NODE_ENV=production vite --mode production",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build --mode production",
    "preview": "vite preview --mode production --port 4173",
    "preview:dev": "vite preview --mode development --port 5174",
    "lint": "eslint .",
    "start": "./run.sh start",
    "start:prod": "./run.sh start --production",
    "start:dev": "./run.sh start --development",
    "stop": "./run.sh stop",
    "status": "./run.sh status",
    "env-status": "./run.sh env-status",
    "logs": "./run.sh logs",
    "test": "npm run test:baseline",
    "test:baseline": "bash scripts/test-baseline.sh",
    "test:baseline:monitor": "node scripts/monitor-baseline.cjs",
    "test:component": "playwright test e2e/add-display-menu/ --config=e2e/add-display-menu/add-display-menu.config.ts",
    "test:full": "playwright test",
    "test:e2e": "playwright test",
    "test:add-display-menu": "playwright test e2e/add-display-menu/ --config=e2e/add-display-menu/add-display-menu.config.ts",
    "test:add-display-menu:ui": "playwright test e2e/add-display-menu/ --config=e2e/add-display-menu/add-display-menu.config.ts --ui",
    "test:add-display-menu:debug": "playwright test e2e/add-display-menu/ --config=e2e/add-display-menu/add-display-menu.config.ts --debug",
    "test:add-display-menu:codegen": "playwright codegen --config=e2e/add-display-menu/add-display-menu.config.ts",
    "test:unit": "node src/test/run-tests.js",
    "test:fuzzy-search": "node src/test/run-tests.js",
    "test:performance": "node src/test/performance.test.js",
    "test:config": "node src/test/config.test.js",
    "test:shorthand": "node src/test/shorthand.test.js",
    "demo:shorthand": "node src/test/demo-shorthand.js",
    "demo:relaxed": "node src/test/demo-relaxed-search.js",
    "test:timing": "node src/test/test-debouncing.js",
    "test:timing-fixed": "node src/test/test-timing-fixed.js"
  },
  "dependencies": {
    "@reiryoku/ctrader-layer": "file:libs/cTrader-Layer",
    "d3": "^7.9.0",
    "interactjs": "^1.10.27",
    "svelte": "^4.2.7",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^24.7.2",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.1.1",
    "puppeteer": "^24.27.0",
    "vite": "^5.4.19"
  }
}
