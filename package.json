{
  "name": "neurosense-fx",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "NODE_ENV=production vite --mode production",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build --mode production",
    "preview": "vite preview --mode production --port 4173",
    "preview:dev": "vite preview --mode development --port 5174",
    "lint": "eslint .",
    "start": "./run.sh start",
    "start:prod": "./run.sh start --production",
    "start:dev": "./run.sh start --development",
    "stop": "./run.sh stop",
    "status": "./run.sh status",
    "env-status": "./run.sh env-status",
    "logs": "./run.sh logs",
    "demo:shorthand": "node src/test/demo-shorthand.js",
    "demo:relaxed": "node src/test/demo-relaxed-search.js",
    "test:timing": "node src/test/test-debouncing.js",
    "test:timing-fixed": "node src/test/test-timing-fixed.js",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome'",
    "test:e2e:performance": "playwright test tests/e2e/performance*.spec.js",
    "test:e2e:validation": "playwright test tests/e2e/coordinate-precision.spec.js tests/e2e/user-interaction-workflows.spec.js tests/e2e/performance-benchmarking.spec.js tests/e2e/memory-management-cleanup.spec.js",
    "test:e2e:validation-quick": "playwright test tests/e2e/coordinate-precision.spec.js --timeout=30000 --retries=0",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install chromium firefox webkit",
    "test:e2e:codegen": "playwright codegen",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:live-trading": "node scripts/validate-live-trading.js",
    "test:live:e2e": "playwright test e2e/complete-trader-workflow.spec.js",
    "test:complete-workflow": "npm run test:live-trading && npm run test:live:e2e",
    "pipeline": "node tests/pipeline/pipeline-runner.js",
    "pipeline:dev": "node tests/pipeline/pipeline-runner.js --environment development",
    "pipeline:prod": "node tests/pipeline/pipeline-runner.js --environment production",
    "pipeline:headed": "node tests/pipeline/pipeline-runner.js --headed",
    "pipeline:quick": "node tests/pipeline/pipeline-runner.js --skip-performance --skip-deployment",
    "pipeline:phase:build": "node tests/pipeline/pipeline-runner.js --phase build",
    "pipeline:phase:deployment": "node tests/pipeline/pipeline-runner.js --phase deployment",
    "pipeline:phase:workflows": "node tests/pipeline/pipeline-runner.js --phase workflows --headed",
    "pipeline:phase:performance": "node tests/pipeline/pipeline-runner.js --phase performance",
    "test:build-validation": "node tests/pipeline/build-validation.spec.js",
    "test:deployment-validation": "node tests/pipeline/deployment-testing.spec.js",
    "test:workflow-validation": "node tests/pipeline/user-workflow-testing.spec.js",
    "test:performance-regression": "node tests/pipeline/performance-regression-testing.spec.js"
  },
  "dependencies": {
    "@reiryoku/ctrader-layer": "file:libs/cTrader-Layer",
    "d3": "^7.9.0",
    "d3-scale": "^4.0.2",
    "interactjs": "^1.10.27",
    "svelte": "^4.2.7",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^24.7.2",
    "@vitest/ui": "^4.0.13",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^27.2.0",
    "playwright": "^1.56.1",
    "prettier": "^3.1.1",
    "puppeteer": "^24.31.0",
    "vite": "^5.4.19",
    "vitest": "^4.0.13"
  }
}
